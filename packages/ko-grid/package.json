        "jquery-ui-dist": "~1.12.1",
        "knockout": "^3.5.1",
        "rollup": "^1.32.1",
        "typescript": "^3.9.7"
    },
    "peerDependencies": {
        "bowser": "^1.9.4",
        "jquery": "^3.5.0",
        "jquery-ui-dist": "~1.12.1",
        "knockout": "^3.5.1"
    }
